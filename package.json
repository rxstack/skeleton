{
  "name": "RxStackSkeletonApplication",
  "version": "0.2.0",
  "description": "RxStack Skeleton - a fully functional application that you can use as the skeleton for your new applications.",
  "private": true,
  "author": "Nikolay Georgiev <symfonist@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/rxstack/skeleton.git"
  },
  "engines": {
    "node": ">=9"
  },
  "keywords": [
    "rxstack"
  ],
  "main": "dist/index",
  "typings": "dist/index",
  "scripts": {
    "dev": "npm run compile && nodemon",
    "cli": "node ./dist/cli.js",
    "cli-dev": "ts-node ./src/cli.ts",
    "compile": "npm run clean && ./node_modules/typescript/bin/tsc -p .",
    "watch": "./node_modules/typescript/bin/tsc -w -p .",
    "clean": "rm -rf dist",
    "lint": "tslint --project tsconfig.json --format stylish",
    "mocha": "mocha --opts mocha.opts",
    "coverage": "nyc npm run mocha",
    "test": "npm run lint && npm run coverage"
  },
  "dependencies": {
    "@rxstack/async-event-dispatcher": "^0.2",
    "@rxstack/configuration": "^0.2",
    "@rxstack/core": "^0.2.3",
    "@rxstack/exceptions": "^0.2",
    "@rxstack/express-server": "^0.2",
    "@rxstack/socketio-server": "^0.2.1",
    "chalk": "^2.4.2",
    "cors": "^2.8.5",
    "express": "^4.16.4",
    "injection-js": "^2.1.0",
    "reflect-metadata": "^0.1.13",
    "socket.io": "^2.2.0"
  },
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/express": "^4.16.1",
    "@types/lodash": "^4.14.123",
    "@types/mocha": "^2.2.48",
    "@types/node": "^11.13.7",
    "@types/serve-static": "^1.7",
    "@types/sinon": "^5.0.7",
    "chai": "3.5.0",
    "del-cli": "^1.1.0",
    "mocha": "^5.2.0",
    "nodemon": "^1.18.11",
    "nyc": "^14.1.0",
    "request": "^2.88.0",
    "request-promise": "^4.2.4",
    "should": "^13.2.3",
    "sinon": "^6.3.5",
    "socket.io-client": "^2.2.0",
    "source-map-support": "^0.4.15",
    "ts-node": "^3.1.0",
    "tslint": "^5.16.0",
    "typescript": "^3.4.5"
  },
  "bugs": {
    "url": "https://github.com/rxstack/skeleton/issues"
  },
  "homepage": "https://github.com/rxstack/skeleton",
  "nodemonConfig": {
    "ignore": [
      "test/*"
    ],
    "delay": "200"
  },
  "nyc": {
    "extension": [
      ".ts"
    ],
    "exclude": [
      "test/*"
    ],
    "reporter": [
      "text-summary",
      "html",
      "lcov"
    ],
    "sourceMap": true,
    "instrument": true
  }
}
